{
  "expo": {
    "name": "Live Groove Finder",
    "slug": "live-groove-finder-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "livegroovefindermobile",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.tjnaiai.livegroovefinder",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Live Groove Finder needs microphone access to identify songs playing around you.",
        "NSLocationWhenInUseUsageDescription": "Live Groove Finder uses your location to find concerts and events near you.",
        "NSPhotoLibraryUsageDescription": "Live Groove Finder needs photo library access to set your profile avatar."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-notifications",
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5bd80169-576a-4d26-9a88-1217492344ff"
      }
    },
    "owner": "tjnaiai"
  }
}
